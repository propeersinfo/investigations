grammar Arithmetic
  rule def_some
    "def"
  end

  rule id
    additive " "* "id"
  end

  rule additive
    multitive ( '+' multitive )*
  end

  rule multitive
    primary ( [*/%] primary )*
  end

  rule primary
    '(' additive ')' / number
  end

  rule number
    '-'? [1-9] [0-9]*
  end
end
